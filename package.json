{
  "name": "baby-log",
  "version": "1.0.0",
  "description": "Baby Logger Web App",
  "main": "sever/server.js",
  "scripts": {
    "build:client": "rollup -c client.rollup.config.js",
    "build:service-worker": "rollup -c service-worker.rollup.config.js",
    "build:file-manifest": "node tools/build-sw.js --dir client/ --dir build/client/ > client/sw-manifest.json",
    "build": "npm run build:file-manifest && npm run build:service-worker && npm run build:client",
    "start": "node server/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Paul Kinlan",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "arg": "^4.1.3",
    "mime": "^2.4.6",
    "node-fetch": "^2.6.0",
    "rollup": "^2.13.1"
  },
  "engines": {
    "node": "13.0.0"
  }
}
