  "version": "0.2.0",
  "configurations": [
    {
      "name": "Tools/Build SW",
      "program": "${workspaceFolder}/tools/build-sw.js",
      "args": ["--dir", "client/", "--dir", "build/client/", "--ignore", "/sw.js"],
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Server",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/server/server.js"
    }
  ]
}